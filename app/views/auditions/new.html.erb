<h1>Apply to Audition for <%= %></h1>

<% if @audition.errors.any? %>
    <% @audition.errors.full_messages.each do |message| %>
    <%= message %><br>
    <% end %>
<% end %>

<%= form_for @audition do |f| %>
    <%= f.label :name %>
    <%= f.text_field :user_id %><br>
    <%= f.label :job %> ID Number
    <%= f.text_field :job_id %> <br>
    <%= f.submit "I want to Audition" %>
<% end %>
